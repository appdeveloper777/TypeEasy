class Producto {
    nombre : string;
    precio : int;

    __constructor(_nombre : string, _precio : int) {
        this.nombre = _nombre;
        this.precio = _precio;
    }

    Mostrar() {
        print(this.nombre);
        print(this.precio);
    }
}

let producto = new Producto("Fruta", 1);

let productos = [
    new Producto("Chocolate", 1),
    new Producto("PanetÃ³n", 2),
    new Producto("Empanada", 3),
    new Producto("Flan", 2),
    new Producto("Ceviche", 2)
];

let filtrados = filter(productos, p => p.precio > 1);

for (let item in filtrados) {
    print("Producto: ");
    item.Mostrar();
}


