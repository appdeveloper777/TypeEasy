class Producto {
  nombre : string;
  precio : int;

  __constructor(nombre : string, precio : int) {
    this.nombre = nombre;
    this.precio = precio;
  }

  Mostrar() {
    print(this.nombre);
    print(this.precio);
  }
}

let productos = from "productos.csv", Producto;

for (let item in productos) {
    print("Producto:");
    item.Mostrar();
}
