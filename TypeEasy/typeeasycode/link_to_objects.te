class Producto {
  nombre : string;
  precio : int;

  __constructor(nombre : string, precio : int) {
    this.nombre = nombre;
    this.precio = precio;
  }

  Mostrar() {
    print(this.nombre);
    print(this.precio);
  }
}

let productos = from "productos.csv", Producto;
let filtrados = filter(productos, p => p.precio < 3); // LINQ
for (let item in filtrados) {
    print("Producto:");
    item.Mostrar();
}
